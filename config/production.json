{
  "environment": "production",
  "server": {
    "type": "fastapi",
    "url": "${API_URL}",
    "port": "${SERVER_PORT}",
    "workers": "${WORKERS}"
  },
  "corsProxy": {
    "enabled": false,
    "url": null
  },
  "lastUpdated": "2025-01-07T20:00:00Z",
  "elasticsearch": {
    "url": "${ELASTICSEARCH_URL}",
    "kibanaUrl": "${KIBANA_URL}",
    "path": "/api/console/proxy?path=traffic-*/_search&method=POST",
    "directConnection": false,
    "corsRequired": false
  },
  "features": {
    "fastapi": true,
    "localServer": false,
    "corsProxy": false,
    "websocket": true,
    "formulaBuilder": true,
    "authentication": true
  },
  "dashboard": {
    "autoLoadCookie": true,
    "skipCookiePrompt": false,
    "refreshInterval": 300
  },
  "security": {
    "allowedOrigins": "${ALLOWED_ORIGINS}",
    "allowedHosts": "${ALLOWED_HOSTS}",
    "enableDocs": false,
    "requireAuth": true
  }
}
